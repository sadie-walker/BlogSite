<%- include("partials/header")%>

<h1>Blogs</h1>

<% blogs.forEach(function(blog){ %>
    <h3><%=blog.title%></h3>
    <img src="<%=blog.image%>">
    <p><%=blog.body%></p>
    <caption><%=blog.dateCreated%></caption>
    <a class="button ui pink basic" href="/blogs/<%=blog._id%>">Read More</a>
<% }) %>


<%- include("partials/footer")%>